<% if flash[:my_errors]%>
    <% flash[:my_errors].each do |er| %><br>
        <%= er %>
        <%end%>
        <%end%>
       
<%=form_for @song do |f| %>
    <%= f.label "Title:" %> 
    <%= f.text_field :title %>
    <%f.label "Realeased:" %>
    <%= f.text_field :realeased %> 
    <%= f.label "Realease_year:" %> 
    <%= f.text_field :release_year %>
    <%= f.label "Artist:" %> 
    <%= f.text_field :artist_name %>
    <%= f.label "Genre:" %> 
    <%= f.text_field :genre %>

    <%= f.submit %> 
    <%end%>